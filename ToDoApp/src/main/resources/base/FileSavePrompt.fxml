<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane maxHeight="-Infinity"
            maxWidth="-Infinity"
            minHeight="-Infinity"
            minWidth="-Infinity"
            prefHeight="162.0"
            prefWidth="200.0"
            stylesheets="@Styles.css"
            xmlns="http://javafx.com/javafx/17"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="base.SavePromptController">
   <children>
      <TextField fx:id="savePath" layoutX="15.0" layoutY="32.0" promptText="File Path" />
      <Label layoutX="14.0" layoutY="14.0" text="Specify location path" />
      <TextField fx:id="fileNameText" layoutX="15.0" layoutY="90.0" promptText="File name" />
      <Label layoutX="14.0" layoutY="72.0" text="Specify file name" />
      <Button fx:id="fileSaveOkBtn" layoutX="156.0" layoutY="126.0" mnemonicParsing="false" text="Ok" />
   </children>
</AnchorPane>
